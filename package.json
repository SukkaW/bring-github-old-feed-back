{
  "name": "bring-github-old-feed-back",
  "version": "0.1.6",
  "description": "Replace GitHub new useless \"For You\" feed with the old \"Following\" one.",
  "repository": {
    "url": "https://github.com/SukkaW/bring-github-old-feed-back"
  },
  "main": "dist/bring-github-old-feed-back.user.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup --configPlugin rollup-plugin-swc3 -c --bundleConfigAsCjs",
    "lint": "eslint --format=sukka .",
    "prerelease": "pnpm run lint && pnpm run build",
    "release": "bumpp -r --all --commit \"release: %s\" --tag \"%s\""
  },
  "keywords": [
    "github"
  ],
  "author": "SukkaW <https://skk.moe>",
  "license": "MIT",
  "devDependencies": {
    "@swc/core": "^1.15.1",
    "@types/greasemonkey": "^4.0.7",
    "bumpp": "^10.3.1",
    "eslint": "^9.39.1",
    "eslint-config-sukka": "^8.0.4",
    "eslint-formatter-sukka": "^8.0.4",
    "rollup": "^4.53.2",
    "rollup-plugin-swc3": "^0.12.1",
    "rollup-plugin-userscript-metablock": "^0.4.3",
    "typed-query-selector": "^2.12.0"
  },
  "packageManager": "pnpm@10.22.0",
  "pnpm": {
    "overrides": {
      "eslint>chalk": "npm:picocolors@^1.1.1",
      "has": "npm:@nolyfill/has@latest"
    },
    "onlyBuiltDependencies": [
      "@swc/core",
      "oxc-resolver"
    ]
  }
}
